#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/twig@1.17.1/node_modules/twig/bin/node_modules:/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/twig@1.17.1/node_modules/twig/node_modules:/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/twig@1.17.1/node_modules:/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/twig@1.17.1/node_modules/twig/bin/node_modules:/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/twig@1.17.1/node_modules/twig/node_modules:/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/twig@1.17.1/node_modules:/home/wess/Documents/4TWIN24-25/Examen-Batiment/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/twigjs" "$@"
else
  exec node  "$basedir/../../bin/twigjs" "$@"
fi
